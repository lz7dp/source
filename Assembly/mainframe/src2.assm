SRC2     START
         TITLE'OTHER WAY - HELLO WORLD'
         PRINT NOGEN
         BALR 10,0
         USING *,10
         B MESS
MESS     WROUT MESSAGE,ERROR,PARMOD=31,VTSUCBA=VTSUPAR
ERROR    NOP 0
         TERM
*
VTSUPAR  VTSUCB MODE=LINE,BELL=YES
MESSAGE  DC Y(ENDMESS-MESSAGE)
         DS 3X
         DC C'HELLO WORLD!'
ENDMESS  EQU *
*
         END
